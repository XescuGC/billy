{
  "extends": "airbnb/base",
    "rules": {
      "key-spacing": [2, { "align": "value" }],
      "no-multi-spaces": [2, { exceptions: { "ImportDeclaration": true } }],
      "spaced-comment": [0],
      "max-len": [0],
      "no-new": [0],
      "no-unused-vars": [1],
      "no-param-reassign": [1],
    }
}
